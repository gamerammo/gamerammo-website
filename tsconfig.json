{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "lib": [
            "dom",
            "ESNext"
        ],
        "sourceMap": true,
        "rootDir": ".",
        "baseUrl": ".",
        "outDir": "dist",
        "jsx": "react",
        "forceConsistentCasingInFileNames": true,
        "importHelpers": true,
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "node",
        "noImplicitReturns": true,
        "declaration": false,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "suppressImplicitAnyIndexErrors": true,
        "noUnusedLocals": true,
        "allowJs": true,
        "skipLibCheck": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "resolveJsonModule": true,
        "strict": true,
        "removeComments": true,
        "noUnusedParameters": false,
        "inlineSources": true,
        "esModuleInterop": true,
        "paths": {
            "@/*": [
                "src/*"
            ],
            "@@/*": [
                "src/.umi/*"
            ],
            "images/*": [
                "src/images/*"
            ],
            "style/*": [
                "src/styles/*"
            ],
        }
    },
    "include": [
        "src/**/*",
        "test/*"
    ],
    "exclude": [
        "node_modules",
    ]
}